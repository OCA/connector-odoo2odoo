<openerp>
<data>

<record id="product_uom_form_view" model="ir.ui.view">
    <field name="name">product.uom.form.inherit</field>
    <field name="model">product.uom</field>
    <field name="inherit_id" ref="product.product_uom_form_view"/>
    <field name="arch" type="xml">
        <form position="inside">
            <group name="odoo2odoo" string="Odoo2Odoo" groups="connector.group_connector_manager">
                <field name="odoo_bind_ids" nolabel="1"/>
            </group>
        </form>
    </field>
</record>

<record id="odoo_product_uom_view_tree" model="ir.ui.view">
    <field name="name">odoo.product.uom.tree</field>
    <field name="model">odoo.product.uom</field>
    <field name="arch" type="xml">
        <tree string="Odoo2Odoo Bindings">
            <field name="backend_id"/>
            <field name="odoo_id"/>
            <field name="external_odoo_id"/>
            <field name="sync_date"/>
        </tree>
    </field>
</record>

</data>
</openerp>
